{
	"name": "Jira Ticket Rank PPS",
	"version": "3.8",
	"manifest_version": 3,

	"description": "The extension simply displays rank for each Jira ticket in the list. Useful when you want to prioritize stories based.",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"action": {
		"default_title": "Jira Ticket Rank PPS",
		"default_popup": "popup.html"
	},
	"author": "Dmytro Bogdanov",
	"homepage_url": "https://github.com/JeromeGin/jira-ticket-rank-PPS",
	"permissions": ["activeTab", "storage", "scripting"],
	"host_permissions": ["http://*/*","https://*/*"]
}
